{
  "name": "tinyvm",
  "version": "0.0.1",
  "description": "An emulator for a 32-bit new computer.",
  "main": "main.js",
  "scripts": {
    "build": "browserify src/export.js -t [ babelify --presets es2015 ] -o temp/tinyvm.js && babel page --out-dir temp && uglifyjs temp/tinyvm.js temp/startup.js --screw-ie8 -c -m -o lib/tinyvm.js",
    "doc": "java -jar doc/plantuml.jar doc/classes.uml",
    "lint": "eslint src",
    "style": "jscs -v src/*.js",
    "genchars": "utils/genchars data/font.png 127 > src/chars.js",
    "test": "babel-tape-runner test/*.js | tap-spec",
    "clean": "rm -r lib/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://andrenho:9abm8vzu@github.com/andrenho/tinyvm.git"
  },
  "keywords": [
    "emulator"
  ],
  "author": "AndrÃ© Wagner",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/andrenho/tinyvm/issues"
  },
  "homepage": "https://github.com/andrenho/tinyvm#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-plugin-uglify": "^1.0.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-tape-runner": "^2.0.0",
    "babelify": "^7.2.0",
    "canvas": "^1.3.10",
    "eslint": "^2.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0",
    "tape-catch": "^1.0.4"
  },
  "dependencies": {
    "babel-runtime": "^6.5.0"
  }
}
